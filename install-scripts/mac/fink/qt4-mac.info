CompileScript: <<
	#!/bin/bash -ev
	./configure -confirm-license -no-framework -prefix %p/lib/%N/ -demosdir %p/share/doc/%N/demos/ -examplesdir %p/share/doc/%N/examples/ -docdir %p/share/doc/%N/
	make
	find %b \( -name '*.pc' -or -name '*.la' \) -print0 | xargs -0 perl -pi -e 's!-framework !-Wl,-framework,!g'
	find %b/plugins -name '*.dylib' | perl -nle '$file = $_; s|%b/||; system("install_name_tool", "-id", "%p/lib/%N/$_", $file);' -
<<
InstallScript: <<
	#!/bin/bash -ev
	make INSTALL_ROOT=%d install
	mv %i/lib/%N/bin/*.app %b
<<
<<
